Funcion mostrarArreglo(miArreglo)
	Definir i Como Entero
	Para i<-1 Hasta 5 Con Paso 1 Hacer
		Escribir "Elemento ",i," = " , miArreglo[i]
	FinPara
FinFuncion

Algoritmo Arreglado
	Definir cantidadDatos Como Entero
	Dimension miArreglo[5] 
	
	miArreglo[1] = "Goku"
	miArreglo[2] = "Krilin"
	miArreglo[3] = "Roshi"
	miArreglo[4] = "Bulma"
	miArreglo[5] = "Trunks"
	
	mostrarArreglo(miArreglo)

FinAlgoritmo



